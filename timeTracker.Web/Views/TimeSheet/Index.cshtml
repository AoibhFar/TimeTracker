@model IEnumerable<timeTracker.Domain.TimeSheet>

@{
    ViewBag.Title = "Index";
}


<h2>TimeSheets</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OwnerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WeekStarting)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WeeklyHours)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.OwnerName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WeekStarting)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WeeklyHours)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
