@model IEnumerable<timeTracker.Domain.Employee>

@{
    ViewBag.Title = "Index";
}

<h2>Employees</h2>

<ul>
    @foreach (var employee in Model)
    {
        <li>@Html.ActionLink(employee.Name, "details", "employee", new {employee.Id}, null)</li>
    }
</ul>
<p>
    @Html.ActionLink("Add a New Employee", "Create")
</p>
